[Unit]
Description=Legacy Printer Application

[Service]
ExecStart=legacy-printer-app server -o admin-group=none -o auth-service=password-auth -o log-file=- -o log-level=info -o server-options=multi-queue,web-interface,raw-socket,web-network,web-remote,web-security,web-tls
ExecStop=legacy-printer-app shutdown
Type=simple

[Install]
WantedBy=multi-user.target
